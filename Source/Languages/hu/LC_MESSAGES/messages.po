#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Could not open attachment file."
msgstr "Nem lehet megnyitni a csatolt fájlt."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Error while adding background to the document."
msgstr "Hiba történt a dokumentumháttér hozzáadása közben."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "FTP server is not specified."
msgstr "Az FTP szerver nincs megadva."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "FTP server user name is not specified."
msgstr "Az FTP szerver felhasználónév nincs megadva."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "FTP server password is not specified."
msgstr "Az FTP szerver jelszava nincs megadva."

