msgstr "A megadott időszerver fiók az aláíráshoz nincs konfigurálva."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Error while signing the document."
msgstr "Hiba történt a dokumentum aláírása során."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "The default printer is invalid."
msgstr "Az alapértelmezett nyomtató nem érvényes."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "The selected printer is invalid."
msgstr "A kiválasztott nyomtató nem érvényes."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Error while printing the file."
msgstr "Hiba történt a fájl nyomtatása során."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "No script file is specified."
msgstr "Szkript fájl nincs megadva."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Script file does not exist."
msgstr "A szkript fájl nem létezik."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Script file contains illegal characters."
msgstr "A szkript fájl érvénytelen karaktereket tartalmaz."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "ScriptFile path is invalid absolute path."
msgstr "A szkript fájl útvonala egy érvénytelen abszolút útvonal."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "ScriptFile path is too long."
msgstr "A szkript fájl útvonala túl hosszú."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "Error while running the script action."
msgstr "Hiba történt a szkript futtatása közben."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
msgid "No SMTP e-mail address is specified."
msgstr "SMTP e-mail cím nincs megadva."

#: pdfforge.PDFCreator.Conversion.Jobs.ErrorCode
